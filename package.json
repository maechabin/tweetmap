{
  "name": "tweetmap",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 5501",
    "build": "ng build --prod",
    "build:ios": "rm -rf ios && ng run tweetmap:build && cap add ios",
    "open:ios": "cap open ios",
    "build:android": "rm -rf android && ng run tweetmap:build && cap add android",
    "open:android": "cap open android",
    "build:electron": "rm -rf electron && ng run tweetmap:build && cap add electron",
    "open:electtron": "cap open electron",
    "test": "ng test",
    "test:ci": "ng test -- --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "server": "ts-node server/server.ts",
    "deploy": "firebase deploy --only hosting:tweet-map"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.4",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~8.2.4",
    "@angular/compiler": "~8.2.4",
    "@angular/core": "~8.2.4",
    "@angular/forms": "~8.2.4",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.4",
    "@angular/platform-browser-dynamic": "~8.2.4",
    "@angular/router": "~8.2.4",
    "@capacitor/android": "^1.3.0",
    "@capacitor/angular": "^1.0.0",
    "@capacitor/core": "^1.3.0",
    "@capacitor/ios": "^1.3.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/express-ws": "^3.0.0",
    "@types/leaflet": "^1.5.1",
    "@types/twit": "^2.2.23",
    "@types/ws": "^6.0.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "hammerjs": "^2.0.8",
    "leaflet": "^1.5.1",
    "tslib": "^1.10.0",
    "twit": "^2.2.11",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.2",
    "@angular/cli": "~8.3.2",
    "@angular/compiler-cli": "~8.2.4",
    "@angular/language-service": "~8.2.4",
    "@capacitor/cli": "^1.3.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "^7.0.1",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
